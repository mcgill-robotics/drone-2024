<sdf version='1.6'>
  <model name='iris_lidar'>
    <include>
      <uri>model://iris</uri>
    </include>

    <!--lidar-->
    <include>
      <uri>model://lidar</uri>
      <pose>0 0 0.15 0 0 0</pose>
    </include>

    <joint name="lidar_joint" type="fixed">
      <parent>iris::base_link</parent>
      <child>lidar::link</child>
    </joint>
  </model>
</sdf>
